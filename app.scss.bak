* {
  margin: 0;
  padding: 0;
}

*,
*::before,
*::after {
  box-sizing: border-box;
}

:root {
  --white: 0, 0%, 100%;
  --light-gray: 212, 45%, 89%;
  --grayish-blue: 220, 15%, 55%;
  --dark-blue: 218, 44%, 22%;
  --blue: 216.23 95.39% 57.45%;
  --light-blue: 216.3 99.01% 60.39%;

  --color-white: hsl(var(--white));
  --color-light-gray: hsl(var(--light-gray));
  --color-grayish-blue: hsl(var(--grayish-blue));
  --color-dark-blue: hsl(var(--dark-blue));
  --color-blue: hsl(var(--blue));
  --color-light-blue: hsl(var(--light-blue));
  --font-size: 15px;
  --font-family: "Outfit", sans-serif;
}

body {
  height: 90vh;
  display: grid;
  place-content: center;
  font-family: var(--font-family);
  background-color: hsl(var(--light-gray));
}

